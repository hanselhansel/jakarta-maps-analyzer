# Google Maps API Configuration
GOOGLE_MAPS_API_KEY=your_api_key_here

# Analysis Parameters
SEARCH_RADIUS=5000  # meters
RATE_LIMIT=10      # requests per second
MAX_RESULTS=60     # maximum results per location

# Grid Search Configuration
GRID_SIZE=0.005    # decimal degrees (approximately 500m)
OVERLAP_FACTOR=1.5 # overlap between search areas

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG_MODE=false

# Database (optional, for future use)
DATABASE_URL=postgresql://user:password@localhost/jakarta_maps

# Export Settings
EXPORT_FORMAT=csv  # csv, json, excel
INCLUDE_METADATA=true

# API Retry Settings
MAX_RETRIES=3
RETRY_DELAY=5  # seconds

# Community Analysis Settings
COMMUNITY_SEARCH_ENABLED=true
COMMUNITY_CATEGORIES="school,hospital,community_center,mosque,market"
